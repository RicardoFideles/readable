    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,sx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTest.js",
      "!src/api/*",
      "!src/reducers/index.js"
    ],
    "snapshotSerializers": [
